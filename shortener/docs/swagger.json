{
    "swagger": "2.0",
    "info": {
        "description": "Создает алиасы для ссылок",
        "title": "Shortener",
        "contact": {},
        "version": "0.0.1"
    },
    "host": "localhost",
    "basePath": "/",
    "paths": {
        "/analytics/{alias}": {
            "get": {
                "description": "Fetches aggregated redirect data for a short URL alias with filters and pagination.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Analytics"
                ],
                "summary": "Get redirect analytics (JSON API)",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Short URL alias",
                        "name": "alias",
                        "in": "path",
                        "required": true
                    },
                    {
                        "type": "string",
                        "default": "2025-01-01T00:00:00Z",
                        "description": "Start date in ISO8601 format",
                        "name": "start_date",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "default": "2025-12-31T23:59:59Z",
                        "description": "End date in ISO8601 format",
                        "name": "end_date",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "default": "user_agent",
                        "description": "Column to filter by (e.g. user_agent)",
                        "name": "filter",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "default": "Mozilla/5.0...",
                        "description": "Value to filter",
                        "name": "value",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "default": 1,
                        "description": "Page number",
                        "name": "page",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/redirect.Agrigated"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    }
                }
            }
        },
        "/s/{alias}": {
            "get": {
                "description": "Redirects user to the original URL. On error, returns JSON.",
                "tags": [
                    "URLs"
                ],
                "summary": "Redirect by alias",
                "parameters": [
                    {
                        "type": "string",
                        "description": "Short URL alias",
                        "name": "alias",
                        "in": "path",
                        "required": true
                    }
                ],
                "responses": {
                    "307": {
                        "description": "Temporary redirect to original URL",
                        "schema": {
                            "type": "string"
                        }
                    },
                    "404": {
                        "description": "Not Found",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    },
                    "503": {
                        "description": "Service Unavailable",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    }
                }
            }
        },
        "/shorten": {
            "post": {
                "description": "Creates a short alias for a given original URL.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "URLs"
                ],
                "summary": "Create a new short URL alias",
                "parameters": [
                    {
                        "description": "Shortening request",
                        "name": "request",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/request.NewShort"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    },
                    "503": {
                        "description": "Service Unavailable",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "redirect.Agrigated": {
            "type": "object",
            "properties": {
                "alias": {
                    "type": "string"
                },
                "redirects": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/redirect.Redirect"
                    }
                },
                "total": {
                    "type": "integer",
                    "format": "int64"
                }
            }
        },
        "redirect.Redirect": {
            "type": "object",
            "properties": {
                "alias": {
                    "type": "string"
                },
                "date": {
                    "type": "string"
                },
                "id": {
                    "type": "integer",
                    "format": "int64"
                },
                "userAgent": {
                    "type": "string"
                }
            }
        },
        "request.NewShort": {
            "type": "object",
            "properties": {
                "alias": {
                    "type": "string"
                },
                "original": {
                    "type": "string"
                }
            }
        },
        "response.Response": {
            "type": "object",
            "properties": {
                "error": {
                    "type": "string"
                },
                "result": {},
                "status": {
                    "type": "string"
                }
            }
        }
    }
}